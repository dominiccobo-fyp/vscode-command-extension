{
	"name": "vscode-context-command",
	"displayName": "vscode-context-command",
	"publisher": "dominiccobo",
	"description": "Context requesting interface",
	"version": "0.8.2",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.contextCommands",
				"title": "More info"
			}
		],
		"configuration": [
			{
				"title": "Global Context Service",
				"properties": {
					"vscode-context-command.connectorPort": {
						"type": "integer",
						"default": 31024,
						"description": "The port on which the connector runs."
					},
					"vscode-context-command.connectorIP": {
						"type": "string",
						"default": "192.168.99.100",
						"description": "The host where the connector instance runs."
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run downloadJar && npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js",
		"downloadJar": "node downloadJar.js",
		"build-extension": "vsce package"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"@types/ws": "^6.0.3",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2",
		"request": "2.88.0",
		"request-promise-native": "1.0.8",
		"@types/request-promise-native": "1.0.17"
	},
	"dependencies": {
		"ws": "^7.2.0",
		"request": "2.88.0",
		"request-promise-native": "1.0.8"
	}
}
